{
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src",
    "qualify-verify": "yarn lint && yarn test",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "start": "react-native start",
    "test": "jest",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "build": "yarn && yarn build:android && yarn build:ios",
    "rebuild": "yarn clean && yarn build",
    "clean": "yarn clean:android && yarn clean:ios && rm -rf node_modules && rm -f yarn.lock",
    "clean:android": "rm -rf android/app/build && rm -rf android/build && rm -rf android/.gradle && rm -rf android/build.gradle-e",
    "clean:ios": "rm -rf ios/build && rm -rf ios/Pods && rm -rf ios/Podfile.lock && rm -rf ios/ReactNativeApp.xcworkspace"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.5",
    "@hookform/resolvers": "^2.8.0",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/drawer": "^6.1.4",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@types/react": "^17.0.19",
    "@types/react-native": "^0.64.13",
    "expo": "~42.0.1",
    "expo-splash-screen": "~0.11.2",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.8.1",
    "native-base": "^3.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^7.14.0",
    "react-native": "~0.63.4",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "^3.6.0",
    "react-native-svg": "12.1.1",
    "react-native-unimodules": "~0.14.5",
    "react-native-web": "~0.13.12",
    "react-redux": "^7.2.4",
    "styled-components": "^5.3.0",
    "styled-system": "^5.1.5",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "7.11.0",
    "@babel/runtime": "7.11.0",
    "@react-native-community/cli-platform-android": "3.1.4",
    "@react-native-community/eslint-config": "1.1.0",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-root-import": "6.5.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.9.0",
    "eslint-import-resolver-babel-plugin-root-import": "1.1.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "2.5.1",
    "gerador-validador-cpf": "3.1.3",
    "husky": "3.1.0",
    "jest": "^27.1.0",
    "jest-expo": "^42.1.0",
    "jetifier": "1.6.6",
    "lint-staged": "9.5.0",
    "metro-react-native-babel-preset": "0.59.0",
    "prettier": "1.19.1",
    "react-dom": "16.13.1",
    "reactotron-react-native": "5.0.0",
    "reactotron-redux": "3.1.3"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globalMock.js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn quality-verify"
    }
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  }
}
